		{
			"name": "/login",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"var jsonData = JSON.parse(responseBody);",
							"tests[\"Access Token is Exist\"] = jsonData.access_token !== undefined",
							"postman.setEnvironmentVariable(\"jwt_token\", jsonData.token.access_token)",
							"postman.setEnvironmentVariable(\"refresh_token\", jsonData.token.refresh_token)"
						],
						"type": "text/javascript"
					}
